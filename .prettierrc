{
  "semi": false,
  "singleQuote": true,
  "jsxSingleQuote": false,
  "tabWidth": 2,
  "useTabs": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "arrowParens": "always",
  "endOfLine": "auto",
  "organizeImportsSkipDestructiveCodeActions": true,
  "plugins": ["prettier-plugin-tailwindcss"]
}
